    "ejs": "^2.6.1",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "marked": "^0.5.2",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.2"
  }
