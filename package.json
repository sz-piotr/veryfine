  "repository": {
    "type": "git",
    "url": "https://github.com/sz-piotr/veryfine.git"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "rm -rf lib && tsc -b tsconfig.build.json",
